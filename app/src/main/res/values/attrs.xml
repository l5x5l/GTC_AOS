<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="loginEditText">
        <attr name="hint_message" format="string"/>
        <attr name="input_type" format="enum">
            <enum name="none" value="0"/>
            <enum name="text" value="1"/>
            <enum name="number" value="2"/>
            <enum name="password" value="129"/>
        </attr>
        <attr name="max_length" format="integer"/>
        <attr name="use_default_delete_button" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="loginIdSelectButton">
        <attr name="login_id_type" format="string"/>
        <attr name="is_default" format="boolean"/>
    </declare-styleable>
</resources>